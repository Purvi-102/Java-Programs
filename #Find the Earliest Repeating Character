class HelloWorld {
  public static void main(String[] args){
    String s = "geksdfrghnbc";
    boolean flag = true;
    for(int i =0;i<s.length()-1;i++){
      if(s.charAt(i)==s.charAt(i+1)){
        System.out.println(s.charAt(i));
        flag = !flag;
        break;
        
      }
    }
    if(flag){
      System.out.println("No repeat");
      
    }
       
    
  }
}
